    T_Break          shift, and go to state 41
    T_Continue       shift, and go to state 42
    T_For            shift, and go to state 43
    T_If             shift, and go to state 44
    T_Return         shift, and go to state 45
    T_Switch         shift, and go to state 46
    T_IntConstant    shift, and go to state 47
    T_FloatConstant  shift, and go to state 48
    T_BoolConstant   shift, and go to state 49
    T_Inc            shift, and go to state 50
    T_Dec            shift, and go to state 51
    T_Identifier     shift, and go to state 52
    '('              shift, and go to state 53
    '+'              shift, and go to state 54
    '-'              shift, and go to state 55
    ';'              shift, and go to state 56
    '{'              shift, and go to state 57
    '}'              shift, and go to state 154

    Var_Ident                     go to state 59
    Pri_Expr                      go to state 60
    Pst_Expr                      go to state 61

   14 Func_Call_Or_Method: . Func_Call_Generic
   15 Func_Call_Generic: . Func_Call_Header_With_Params ')'
   16                  | . Func_Call_Header_No_Params ')'
   17 Func_Call_Header_No_Params: . Func_Call_Header T_Void
   18                           | . Func_Call_Header
   19 Func_Call_Header_With_Params: . Func_Call_Header Assign_Expr
   20                             | . Func_Call_Header_With_Params ',' Assign_Expr
   21 Func_Call_Header: . Func_Id '('
   22 Func_Id: . Non_Prim_Type
   23 Unary_Expr: . Pst_Expr
   24           | . T_Inc Unary_Expr
   25           | . T_Dec Unary_Expr
   26           | . Unary_Op Unary_Expr
   27 Unary_Op: . '+'
   28         | . '-'
   29 Mult_Expr: . Unary_Expr
   30          | . Mult_Expr '*' Unary_Expr
   31          | . Mult_Expr '/' Unary_Expr
   32 Add_Expr: . Mult_Expr
   33         | . Add_Expr '+' Mult_Expr
   34         | . Add_Expr '-' Mult_Expr
   35 Shift_Expr: . Add_Expr
   36 Rel_Expr: . Shift_Expr
   37         | . Rel_Expr '<' Shift_Expr
   38         | . Rel_Expr '>' Shift_Expr
   39         | . Rel_Expr T_LessEqual Shift_Expr
   40         | . Rel_Expr T_GreaterEqual Shift_Expr
   41 Eq_Expr: . Rel_Expr
   42        | . Eq_Expr T_Equal Rel_Expr
   43        | . Eq_Expr T_NotEqual Rel_Expr
   44 And_Expr: . Eq_Expr
   45 Xor_Expr: . And_Expr
   46 Inc_Or_Expr: . Xor_Expr
   47 Log_And_Expr: . Inc_Or_Expr
   48             | . Log_And_Expr T_And Inc_Or_Expr
   49 Log_Xor_Expr: . Log_And_Expr
   50 Log_Or_Expr: . Log_Xor_Expr
   51            | . Log_Or_Expr T_Or Log_Xor_Expr
   52 Cond_Expr: . Log_Or_Expr
   53 Assign_Expr: . Cond_Expr
   54            | . Unary_Expr Assign_Op Assign_Expr
   60 Expr: . Assign_Expr
   61 Decl: . Func_Proto ';'
   62     | . Init_Decl_List ';'
   63 Func_Proto: . Func_Declr ')'
   64 Func_Declr: . Func_Hdr
   65           | . Func_Hdr_With_Param
   66 Func_Hdr_With_Param: . Func_Hdr Param_Decl
   67                    | . Func_Hdr_With_Param ',' Param_Decl
   68 Func_Hdr: . Fully_Spec_Type Identifier '('
   74 Init_Decl_List: . Single_Decl
   75 Single_Decl: . Fully_Spec_Type T_Identifier
   76 Fully_Spec_Type: . Type_Spec
   77 Type_Spec: . Type_Spec_Nonarr
   78 Type_Spec_Nonarr: . T_Void
   79                 | . T_Float
   80                 | . T_Int
   81                 | . T_Bool
   82                 | . Non_Prim_Type
   83 Non_Prim_Type: . T_Vec2
   84              | . T_Vec3
   85              | . T_Vec4
   86              | . T_Mat2
   87              | . T_Mat3
   88              | . T_Mat4
   89 Decl_Stmt: . Decl
   90 Stmt: . Compd_Stmt_With_Scope
   91     | . Simple_Stmt
   96 Simple_Stmt: . Decl_Stmt
   97            | . Expr_Stmt
   98            | . Select_Stmt
   99            | . Switch_Stmt
  100            | . Iter_Stmt
  101            | . Jump_Stmt
  102 Jump_Stmt: . T_Continue ';'
  103          | . T_Break ';'
  104          | . T_Return ';'
  105          | . T_Return Expr ';'
  106 Compd_Stmt_With_Scope: . '{' '}'
  107                      | . '{' Stmt_List '}'
  107                      | '{' Stmt_List . '}'
  111 Stmt_List: Stmt_List . Stmt
  112 Expr_Stmt: . ';'
  113          | . Expr ';'
  114 Select_Stmt: . T_If '(' Expr ')' Select_Rest_Stmt
  119 Switch_Stmt: . T_Switch '(' Expr ')' '{' Switch_Stmt_List '}'
  127 Iter_Stmt: . T_While '(' Cond ')' Stmt_No_New_Scope
  128          | . T_For '(' Expr_Stmt Cond ';' ')' Stmt_No_New_Scope
  129          | . T_For '(' Expr_Stmt Cond ';' Expr ')' Stmt_No_New_Scope

    T_Void           shift, and go to state 1
    T_Bool           shift, and go to state 2
    T_Float          shift, and go to state 3
    T_Int            shift, and go to state 4
    T_Vec2           shift, and go to state 5
    T_Vec3           shift, and go to state 6
    T_Vec4           shift, and go to state 7
    T_Mat2           shift, and go to state 8
    T_Mat3           shift, and go to state 9
    T_Mat4           shift, and go to state 10
